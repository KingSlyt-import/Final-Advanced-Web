    <section class="hero-banner d-flex align-items-center">
        <div class="container text-center">
            <h2>Trang thông tin cá nhân</h2>
            <nav aria-label="breadcrumb" class="banner-breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="/index">Trang chủ</a></li>
                </ol>
            </nav>
        </div>
    </section>


    <section class="about-area">
        <div class="container">
            <div class="row align-items-center">
                <form class="userinfo">
                    <div class="col-lg-12">
                        <h1 class="mt-4">Thông tin người dùng</h1>
                        <hr class="my-4">
                        <div class="row">
                            <div class="col-12">
                                <div class="form-group focused">
                                    <label class="form-control-label">Họ tên</label>
                                    <input type="text" class="form-control form-control-alternative"
                                        placeholder="Họ tên">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="form-group">
                                    <label class="form-control-label">Địa chỉ email</label>
                                    <input type="text" class="form-control form-control-alternative"
                                        placeholder="Email">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-8">
                                <div class="form-group">
                                    <label class="form-control-label">Sinh nhật</label>
                                    <input type="date" class="form-control form-control-alternative">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-6">
                                <div class="form-group">
                                    <label class="form-control-label">Vai trò</label>
                                    <input type="text" class="form-control form-control-alternative">
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="dropdown show">
                                    <a class="btn btn-secondary dropdown-toggle" href="#" role="button"
                                        id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true"
                                        aria-expanded="false">
                                        Trạng thái
                                    </a>

                                    <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                                        <a class="dropdown-item" href="#">Đã xác minh</a>
                                        <a class="dropdown-item" href="#">Chờ xác minh</a>
                                        <a class="dropdown-item" href="#">Khóa tài khoản</a>
                                        <a class="dropdown-item" href="#">Bổ sung CMND</a>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <hr class="my-4">
                        <h1 class="mt-4">Thông tin liên hệ</h1>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="form-group focused">
                                    <label class="form-control-label">Địa chỉ</label>
                                    <input class="form-control form-control-alternative" placeholder="Địa chỉ"
                                        type="text">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-8">
                                <div class="form-group">
                                    <label class="form-control-label">Số điện thoại</label>
                                    <input type="text" class="form-control form-control-alternative"
                                        placeholder="Số điện thoại">
                                </div>
                            </div>
                        </div>
                        <hr class="my-4">
                        <h1 class="mt-4">Thông tin giao dịch</h1>
                        <div class="row">
                            <div class="col-md-5">
                                <div class="form-group">
                                    <label class="form-control-label">Số dư</label>
                                    <input type="text" class="form-control form-control-alternative"
                                        placeholder="Số dư">
                                </div>
                            </div>
                            <div class="col-md-7">
                                <div class="form-group">
                                    <label class="form-control-label">Số lần giao dịch</label>
                                    <input type="text" class="form-control form-control-alternative"
                                        placeholder="Số lần giao dịch">
                                </div>
                            </div>
                        </div>
                </form>
            </div>

        </div>
    </section>
    <footer class="footer-area">
        <div class="container">
            <div class="row align-items-center text-center">

